process {
    withName: 'CONTAMINANT_FILTER_SRNAS:BLAT.*' {
        ext.args = '-out=blast8'
        ext.prefix = {"${meta.id}_${meta2.id}"}
        tag = {"${meta.id} ${meta2.id}"}
    }

    withName: 'CONTAMINANT_FILTER_SRNAS:GAWK.*' {
        ext.prefix = {"significant_hits_${meta.id}"}
    }

    withName: 'CONTAMINANT_FILTER_SRNAS:SEQKIT_GREP.*' {
        ext.prefix = {"filtered_${meta.id}"}
        ext.args = '-v'
    }
}
